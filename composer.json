{
    "name": "elvisthermiranda/rabbitmq-laravel",
    "description": "Um pacote para integrar o rabbitmq ao job do laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ElvistherMiranda\\RabbitmqLaravel\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/queue": "^10.0|^11.0|^12.0",
        "illuminate/container": "^10.0|^11.0|^12.0",
        "php-amqplib/php-amqplib": "^3.7"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ElvistherMiranda\\RabbitmqLaravel\\RabbitServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
